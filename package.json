{
  "name": "ai-build-flow",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": ["apps/*","packages/*"],
  "scripts": {
    "dev:web": "npm --workspace apps/web run dev",
    "dev:mobile": "npm --workspace apps/mobile run start",
