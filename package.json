{
  "name": "ai-build-flow",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "apps/web",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm --workspace apps/web run dev",
    "build:web": "npm --workspace apps/web run build",
    "start:web": "npm --workspace apps/web run start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@types/marked": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.21"
  },
  "dependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "marked": "12.0.2",
    "typescript": "^5.9.2"
  }
}
